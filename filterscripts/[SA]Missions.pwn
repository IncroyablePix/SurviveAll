	print("- Loaded");
	print("...");
	//---INITILIALISATION---//
	for(new i = 0; i < MAX_PLAYERS; i ++)
	{
	    if(IsPlayerConnected(i)) GetPlayerName(i, sPlayerName[i], MAX_PLAYER_NAME + 1);
	    else format(sPlayerName[i], MAX_PLAYER_NAME + 1, "%s", "_");
		pMissionInfos[i][dMissionID] = NO_MISSION;
		pMissionInfos[i][dMissionVehicle][0] = INVALID_VEHICLE_ID;
		pMissionInfos[i][dMissionVehicle][1] = INVALID_VEHICLE_ID;
		pMissionInfos[i][dMultiMissionSlot] = -1;
		pMissionInfos[i][dTempsRestant] = -1;
