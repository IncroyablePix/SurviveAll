public OnDynamicObjectMoved(objectid)
{
	for(new i = 0; i < sizeof(pMission); i ++)
	{
	    if(objectid == pMission[i][dMissionInfos][0])
	    {
		    if(pMission[i][dMissionID] == MISSION_AIR_BALLOON)
			{
	    		OnCinematicGoesOn(pMission[i][dPlayers][0], 24, 9, 0);
	    		break;
			}
		    else if(pMission[i][dMissionID] == MISSION_AIR_BALLOON_2)
			{
	    		OnCinematicGoesOn(pMission[i][dPlayers][0], 24, 10, 0);
				break;
			}
			else if(pMission[i][dMissionID] == MISSION_LONG_WINGS)
			{
	    		OnCinematicGoesOn(pMission[i][dPlayers][0], 25, 10, 0);
				break;
			}
		}
	}
	return 1;
}

public OnPlayerShootDynamicObject(playerid, weaponid, objectid, Float:x, Float:y, Float:z)
{
	if(objectid == INVALID_OBJECT_ID) return 1;
	//---
	for(new i = 0; i < MAX_PLAYERS / 2; i ++)
	{
	    if(pMission[i][dMissionID] == MISSION_AIR_BALLOON || pMission[i][dMissionID] == MISSION_AIR_BALLOON_2 && objectid == pMission[i][dMissionInfos][0])
		{
			pMission[i][dMissionInfos][1] -= CallRemoteFunction("GetWeaponDamage", "i", weaponid);
			CallRemoteFunction("UpdateHealthBar", "iiissssss", pMission[i][dPlayers][0], pMission[i][dMissionInfos][1], 30000, "Air Balloon", "Montgolfiere", "Bambi", "Bambi", "Bambi", "Bambi");
			CallRemoteFunction("UpdateHealthBar", "iiissssss", pMission[i][dPlayers][1], pMission[i][dMissionInfos][1], 30000, "Air Balloon", "Montgolfiere", "Bambi", "Bambi", "Bambi", "Bambi");
    		if(pMission[i][dMissionInfos][1] <= 0) OnCinematicGoesOn(pMission[i][dPlayers][0], 24, 13, 0);
    		break;
		}
	    else if(pMission[i][dMissionID] == MISSION_LONG_WINGS && objectid == pMission[i][dMissionInfos][0])
		{
			pMission[i][dMissionInfos][1] -= CallRemoteFunction("GetWeaponDamage", "i", weaponid);
			CallRemoteFunction("UpdateHealthBar", "iiissssss", pMission[i][dPlayers][0], pMission[i][dMissionInfos][1], 35000, "Plane", "Avion", "Bambi", "Bambi", "Bambi", "Bambi");
			CallRemoteFunction("UpdateHealthBar", "iiissssss", pMission[i][dPlayers][1], pMission[i][dMissionInfos][1], 35000, "Plane", "Avion", "Bambi", "Bambi", "Bambi", "Bambi");
    		if(pMission[i][dMissionInfos][1] <= 0) OnCinematicGoesOn(pMission[i][dPlayers][0], 25, 13, 0);
    		break;
		}
	}
	return 1;
}

