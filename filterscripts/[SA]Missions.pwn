public StartPlayerMission(playerid)//Return true si on commence une mission - false si aucune mission ne peut démarrer
{
	if(bCinematic[playerid] || bTextDrawShow[playerid]) return false;
	if(IsPlayerInRangeOfPoint(playerid, 5.0, 146.9200, 1835.7262, 17.6460))//Scientifique Area
	{
		if(GetPlayerMission(playerid) == NO_MISSION)
		{
			OnCinematicGoesOn(playerid, 14, 0, 0);
			OnPlayerStartsMission(playerid);
			return true;
		}
		else if(GetPlayerMission(playerid) == MISSION_UFO_SHOOT && pMissionInfos[playerid][dInfo][0] >= 25)
		{
			OnCinematicGoesOn(playerid, 14, 8, 0);
		    return true;
		}
	}
	else if(IsPlayerInRangeOfPoint(playerid, 5.0, 738.8403, 2062.3645, 9.2171))//Bob
	{
		if(GetPlayerMission(playerid) == NO_MISSION)
		{
			OnCinematicGoesOn(playerid, 29, 0, 0);
			OnPlayerStartsMission(playerid);
			return true;
