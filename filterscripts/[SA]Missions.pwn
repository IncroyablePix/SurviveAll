		pMissionInfos[i][dInfo][3] = 0;
		//REGGAE SHARK
		for(new j = 0; j < 11; j ++) oCollect[i][j] = INVALID_OBJECT_ID;
		oMeduse[i][0] = INVALID_OBJECT_ID;
		oMeduse[i][1] = INVALID_OBJECT_ID;
		oJoint[i][0] = INVALID_OBJECT_ID;
		oJoint[i][1] = INVALID_OBJECT_ID;
		oReggaeShark[i][0] = INVALID_OBJECT_ID;
		oReggaeShark[i][1] = INVALID_OBJECT_ID;
