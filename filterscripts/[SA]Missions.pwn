public OnPlayerPickupMisison(playerid, objectid)
{
	if(objectid == 68 && pMissionInfos[playerid][dMissionID] == MISSION_CARIBOO_1)
	{
		OnCinematicGoesOn(playerid, 12, 16, CallRemoteFunction("GetPlayerItemAmount", "ii", playerid, 68));
		return true;
	}
	else if(objectid == 68 && pMissionInfos[playerid][dMissionID] == MISSION_BIRCH_HUNTING)
	{
		OnCinematicGoesOn(playerid, 110, 35, CallRemoteFunction("GetPlayerItemAmount", "ii", playerid, 68));
		return true;
	}
	else if(objectid == 67 && pMissionInfos[playerid][dMissionID] == MISSION_TMNT_2_COLLECT)
	{
		OnCinematicGoesOn(playerid, 21, 11, CallRemoteFunction("GetPlayerItemAmount", "ii", playerid, 67));
		return true;
	}
	return false;
}

