public OnFilterScriptExit()
{
	print("--------------------------------------");
	print("Survive-All missions - [Pix]");
	print("- Unloaded");
	print("--------------------------------------\n");
	//----------------------//
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i ++)
	{
		if(GetPlayerMission(i)) StopPlayerMission(i, true);
		if(bTextDrawShow[i]) FadeMissionText(i, 7, true);
		//---
		ShowPlayerMissionBlips(i, false);
	}
	//----------------------//
	TextDrawDestroy(tBlackScreen);
	TextDrawDestroy(tTitle[0]);
	TextDrawDestroy(tTitle[1]);
	//----------------------//
	#if defined PROFILING
 	Profiler_Stop();
	#endif
	return 1;
}

public OnPlayerConnect(playerid)
{
	//---Nom joueur
	GetPlayerName(playerid, sPlayerName[playerid], MAX_PLAYER_NAME + 1);
	bDrakeTalked[playerid] = false;
	ShowPlayerMissionBlips(playerid, true);
    return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
	if(GetPlayerMission(playerid)) StopPlayerMission(playerid, false);
	if(IsPlayerOnCinematic(playerid)) bCinematic[playerid] = false;
	return 1;
}

public OnPlayerSpawn(playerid)
{
	ShowPlayerMissionBlips(playerid, true);
	return 1;
}

public OnVehicleDeath(vehicleid, killerid)
{
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i ++)
	{
	    if(pMissionInfos[i][dMissionID] == MISSION_GAS && (vehicleid == pMissionInfos[i][dMissionVehicle][0] || vehicleid == pMissionInfos[i][dMissionVehicle][1]))
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the truck.", "~r~Vous avez détruit le camion.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	    else if((pMissionInfos[i][dMissionID] == MISSION_WAVES_DRIVING || pMissionInfos[i][dMissionID] == MISSION_WAVES_ZOMBIES) && vehicleid == pMissionInfos[i][dMissionVehicle][0])
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the truck.", "~r~Vous avez détruit le camion.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	    else if(MISSION_CABOOSE_DRIVE >= pMissionInfos[i][dMissionID] >= MISSION_CABOOSE_FIND && vehicleid == pMissionInfos[i][dMissionVehicle][0])
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the truck.", "~r~Vous avez détruit le camion.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	    else if(MISSION_WINKY_LEAKS_CAR >= pMissionInfos[i][dMissionID] >= MISSION_WINKY_LEAKS_BACK && vehicleid == pMissionInfos[i][dMissionVehicle][0] && vehicleid != pMissionInfos[i][dMissionVehicle][1])
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the car.", "~r~Vous avez détruit la voiture.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	    else if(MISSION_BIRCH_ENDING >= pMissionInfos[i][dMissionID] >= MISSION_BIRCH_CAR && vehicleid == pMissionInfos[i][dMissionVehicle][0])
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the car.", "~r~Vous avez détruit la voiture.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	    else if(MISSION_ANGEL_ESCAPE == pMissionInfos[i][dMissionID] && vehicleid == pMissionInfos[i][dMissionVehicle][0])
	    {
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the car.", "~r~Vous avez détruit la voiture.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
		else if(MISSION_EVERYDAY_AMBULANCE >= pMissionInfos[i][dMissionID] > MISSION_EVERYDAY_GO && vehicleid == pMissionInfos[i][dMissionVehicle][1])
		{
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the ambulance.", "~r~Vous avez détruit l'ambulance.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
		else if(MISSION_WEAK_DRIVE >= pMissionInfos[i][dMissionID] >= MISSION_WEAK_NEXT && vehicleid == pMissionInfos[i][dMissionVehicle][1])
		{
			StopPlayerMission(i, false);
			CallRemoteFunction("ShowPlayerTextInfo", "iissssss", i, 5000, "~r~You destroyed the car.", "~r~Vous avez détruit la voiture.", "Espagnol", "Portugais", "Italien", "Allemand");
			break;
		}
	}
	return 1;
}

public OnPlayerDeath(playerid, killerid, reason)
{
	if(pMissionInfos[playerid][dMissionID] != NO_MISSION)
	{
		CallRemoteFunction("ShowPlayerTextInfo", "iissssss", playerid, 5000, "~r~You are dead.", "~r~Vous êtes mort.", "Espagnol", "Portugais", "Italien", "Allemand");
	    StopPlayerMission(playerid, false);
	}
	return 1;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
	new vehicleid = GetPlayerVehicleID(playerid);
	//---SORTIR
	if(oldstate == PLAYER_STATE_DRIVER && newstate == PLAYER_STATE_ONFOOT)
