 	if(dCurrentAmmo < maxammo && (CallRemoteFunction("HasPlayerSameTypeWeapon", "ii", playerid, weaponid) != 0 || CallRemoteFunction("GetPlayerNextFreeWeaponSlot", "i", playerid) != 0))
 	{
 	    CallRemoteFunction("GivePlayerWeaponEx", "iii", playerid, dCurrentWeapon == 0 ? weaponid : dCurrentWeapon, (ammo + dCurrentAmmo > maxammo ? maxammo - dCurrentAmmo : ammo));
 	}
}
