MissionAccomplished(playerid, const earning1[], const earning2[], const earning3[])
{
	if(bTextDrawShow[playerid]) return 0;
	tMissionPassed[playerid] = CreatePlayerTextDraw(playerid, 320.000000, 170.000000, "Mission Accomplie !~n~~w~Exp+");
	PlayerTextDrawAlignment(playerid, tMissionPassed[playerid], 2);
	PlayerTextDrawBackgroundColor(playerid, tMissionPassed[playerid], 255);
	PlayerTextDrawFont(playerid, tMissionPassed[playerid], 3);
	PlayerTextDrawLetterSize(playerid, tMissionPassed[playerid], 1.200000, 4.000000);
	PlayerTextDrawColor(playerid, tMissionPassed[playerid], 0xE89E27FF);
	PlayerTextDrawSetOutline(playerid, tMissionPassed[playerid], 1);
	PlayerTextDrawSetProportional(playerid, tMissionPassed[playerid], 1);
	PlayerTextDrawSetSelectable(playerid, tMissionPassed[playerid], 0);
	//---
	new result[256];
	switch(CallRemoteFunction("GetPlayerLanguage", "i", playerid))
	{
		case LANGUAGE_EN: strcat(result, "Mission passed!");
		case LANGUAGE_FR: strcat(result, "Mission accomplie !");
		case LANGUAGE_ES: strcat(result, "Espagnol");
		case LANGUAGE_PG: strcat(result, "Portugais");
		case LANGUAGE_IT: strcat(result, "Italien");
		case LANGUAGE_DE: strcat(result, "Allemand");
	}
	if(strlen(earning1) != 0)
	{
		strcat(result, "~n~~w~");
		strcat(result, earning1);
    }
	if(strlen(earning2) != 0)
 	{
	 	strcat(result, "~n~");
		strcat(result, earning2);
	}
	if(strlen(earning3) != 0)
	{
		strcat(result, "~n~");
		strcat(result, earning3);
	}
	bTextDrawShow[playerid] = true;
	PlayerTextDrawSetString(playerid, tMissionPassed[playerid], result);
	PlayerTextDrawShow(playerid, tMissionPassed[playerid]);
	SetTimerEx("FadeMissionText", 5500, false, "ii", playerid, 7);
	return 1;
}

