			dMissionSlot = i;
			break;
		}
	}
	if(dMissionSlot == -1) return 0;
	//---
	GetPMissionSlot(playerid) = dMissionSlot;
	pMission[dMissionSlot][dPlayers][0] = playerid;
	pMission[dMissionSlot][dGroupSize] = 1;
	//---
	if(mates > 0)
	{
	    new Float:x, Float:y, Float:z;
	    GetPlayerPos(playerid, x, y, z);
		do
		{
	        for(new i = 0, j = GetPlayerPoolSize(); i <= j; i ++)
	        {
	            if(CallRemoteFunction("GetPlayerGroup", "i", i) == groupid && GetPMissionSlot(i) == -1 && playerid != i && GetPlayerMission(i) == NO_MISSION && !bCinematic[i] && IsPlayerInRangeOfPoint(i, 5.0, x, y, z))
	            {
	            	GetPMissionSlot(i) = dMissionSlot;
					pMission[dMissionSlot][dPlayers][pMission[dMissionSlot][dGroupSize]] = i;
					pMission[dMissionSlot][dGroupSize] ++;
	            }
	        }
		}
		while(pMission[dMissionSlot][dGroupSize] < minimates || (pMission[dMissionSlot][dPlayers][pMission[dMissionSlot][dGroupSize] - 1] == INVALID_PLAYER_ID && mates == pMission[dMissionSlot][dGroupSize]));
		/*for(new k = 1; k < mates; k ++)
	    {
	        for(new i = 0, j = GetPlayerPoolSize(); i <= j; i ++)
	        {
	            if(playerid != i && GetPMissionSlot(i) == -1 && CallRemoteFunction("GetPlayerGroup", "i", i) == groupid && GetPlayerMission(i) == NO_MISSION && !bCinematic[i] && IsPlayerInRangeOfPoint(i, 5.0, x, y, z))
	            {	
	            	GetPMissionSlot(i) = dMissionSlot;
					pMission[dMissionSlot][dPlayers][1 + k] = i;
					pMission[dMissionSlot][dGroupSize] ++;
	            }
	        }
	    }*/
	}
	if(pMission[dMissionSlot][dGroupSize] < minimates)
	{
	    ClearMissionGroup(dMissionSlot);
	    return 0;
	}
	//---
	printf("Test: %d", pMission[dMissionSlot][dPlayers][0]);
	printf("Test: %d", pMission[dMissionSlot][dPlayers][1]);
	printf("Test: %d", pMission[dMissionSlot][dPlayers][2]);
	printf("Test: %d", pMission[dMissionSlot][dPlayers][3]);
	printf("Test: %d", pMission[dMissionSlot][dPlayers][4]);
	printf("GroupSize: %d", pMission[dMissionSlot][dGroupSize]);
	return pMission[dMissionSlot][dGroupSize];
}

AreMissionMatesInRange(playerid, Float:distance)
{
	if(GetPMissionSlot(playerid) == -1) return true;
	new Float:x, Float:y, Float:z;
	GetPlayerPos(playerid, x, y, z);
	for(new i = 0; i < 5; i ++)
	{
		if(pMission[GetPMissionSlot(playerid)][dPlayers][i] != playerid && pMission[GetPMissionSlot(playerid)][dPlayers][i] != INVALID_PLAYER_ID)
		{
		    if(!IsPlayerInRangeOfPoint(pMission[GetPMissionSlot(playerid)][dPlayers][i], distance, x, y, z))
			{
				return false;
			}
		}
	}
	return true;
}

ClearMissionGroup(slot)
{
	for(new i = 0; i < 5; i++)
	{
		if(pMission[slot][dPlayers][i] != INVALID_PLAYER_ID)
		{
			pMissionInfos[pMission[slot][dPlayers][i]][dMissionID] = NO_MISSION;
			pMissionInfos[pMission[slot][dPlayers][i]][dMultiMissionSlot] = -1;
		}
	}
	//---
	pMission[slot][dPlayers][0] = INVALID_PLAYER_ID;
    pMission[slot][dPlayers][1] = INVALID_PLAYER_ID;
    pMission[slot][dPlayers][2] = INVALID_PLAYER_ID;
    pMission[slot][dPlayers][3] = INVALID_PLAYER_ID;
    pMission[slot][dPlayers][4] = INVALID_PLAYER_ID;
    pMission[slot][dMissionID] = NO_MISSION;
    pMission[slot][dMultiVehicle][0] = INVALID_VEHICLE_ID;
    pMission[slot][dMultiVehicle][1] = INVALID_VEHICLE_ID;
    pMission[slot][dMissionInfos][0] = 0;
    pMission[slot][dMissionInfos][1] = 0;
    pMission[slot][dMissionInfos][2] = 0;
    pMission[slot][dMissionInfos][3] = 0;
    pMission[slot][dTime] = 0;
}

