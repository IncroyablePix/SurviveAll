public FadeMissionText(playerid, fade, bool:success)
{
	new hShades[7];
	if(success) hShades = {0xE89E27DD, 0xE89E27BB, 0xE89E2799, 0xE89E2777, 0xE89E2755, 0xE89E2733, 0xE89E2711};
	else hShades = {0xCC0000DD, 0xCC0000BB, 0xCC000099, 0xCC000077, 0xCC000055, 0xCC000033, 0xCC000011};
	switch(fade)
	{
	    case 0 .. 6:
	    {
			PlayerTextDrawColor(playerid, tMissionPassed[playerid], hShades[fade]);
			SetTimerEx("FadeMissionText", 100, false, "ih", playerid, fade + 1);
	    }
	    case 7:
	    {
	        bTextDrawShow[playerid] = false;
	        PlayerTextDrawHide(playerid, tMissionPassed[playerid]);
	        PlayerTextDrawDestroy(playerid, tMissionPassed[playerid]);
	        tMissionPassed[playerid] = PlayerText:INVALID_TEXT_DRAW;
	    }
	}
}

public OnPlayerKillsMob(playerid, mobtype)
{
	if(mobtype == MOB_BAMBIKING)//ROI DES BAMBIS
	{
        if(GetPlayerMission(playerid) == MISSION_CARIBOO_2_HUNT) OnCinematicGoesOn(playerid, 13, 12, 0);
	}
	else if(mobtype == MOB_UFO)//UFO
	{
        if(GetPlayerMission(playerid) == MISSION_UFO_SHOOT) OnCinematicGoesOn(playerid, 14, 7, 0);
	}
	else if(mobtype == MOB_BLOPT)//BLOPT
	{
        if(GetPlayerMission(playerid) == MISSION_DAT_BLOPT) OnCinematicGoesOn(playerid, 16, 5, 0);
	}
	else if(mobtype == MOB_TMNT)//TORTUE
	{
        if(GetPlayerMission(playerid) == MISSION_TMNT_1_KILL) OnCinematicGoesOn(playerid, 20, 10, 0);
	}
	else if(mobtype == MOB_BAMBI)//BAMBI
	{
        if(GetPlayerMission(playerid) == MISSION_BIRCH_FIRST) OnCinematicGoesOn(playerid, 110, 33, 0);
	}
	else if(mobtype == MOB_ZOMBIE)//ZOMBIE
	{
	    if(GetPlayerMission(playerid) == MISSION_GEORGIAN_KILL) OnCinematicGoesOn(playerid, 23, 11, 0);
	    else if(GetPlayerMission(playerid) == MISSION_STREETS_DEMISE_2 || GetPlayerMission(playerid) == MISSION_STREETS_DEMISE_4 || GetPlayerMission(playerid) == MISSION_STREETS_DEMISE_6 || GetPlayerMission(playerid) == MISSION_STREETS_DEMISE_8)
		{
	    	if(pMission[GetPMissionSlot(playerid)][dPlayers][0] == playerid) OnCinematicGoesOn(playerid, 26, 23, 0);
		}
		else if(GetPlayerMission(playerid) == MISSION_TWD_2 || GetPlayerMission(playerid) == MISSION_TWD_4 || GetPlayerMission(playerid) == MISSION_TWD_6 || GetPlayerMission(playerid) == MISSION_TWD_8) OnCinematicGoesOn(playerid, 27, 29, 0);
		else if(GetPlayerMission(playerid) == MISSION_LLS_2 || GetPlayerMission(playerid) == MISSION_LLS_4 || GetPlayerMission(playerid) == MISSION_LLS_6 || GetPlayerMission(playerid) == MISSION_LLS_8) OnCinematicGoesOn(playerid, 30, 29, 0);
		else if(GetPlayerMission(playerid) == MISSION_MASHA_KILL || GetPlayerMission(playerid) == MISSION_MASHA_CAR) OnCinematicGoesOn(playerid, 112, 19, 0);
	}
}

