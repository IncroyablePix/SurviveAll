    MissionFailed(playerid);
	switch(pMissionInfos[playerid][dMissionID])
	{
	    case MISSION_GAS://A drop of gas
	    {
			pMissionInfos[playerid][dMissionID] = NO_MISSION;
			RemovePlayerFromVehicle(playerid);
			for(new i = 0, j = GetPlayerPoolSize(); i <= j; i ++) SetVehicleParamsForPlayer(pMissionInfos[playerid][dMissionVehicle][0], i, 0, true);
			if(reloadfs)
			{
				CallRemoteFunction("DestroyVehicleTimer", "i", pMissionInfos[playerid][dMissionVehicle][0]);
